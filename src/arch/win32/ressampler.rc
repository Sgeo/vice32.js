// template of dialog for all languages

IDD_SAMPLER_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 122
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT           "", IDC_SAMPLER_DEVICE_LABEL,
                    9, 9, 50, 8
    COMBOBOX        IDC_SAMPLER_DEVICE,
                    65, 7, 120, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT           "", IDC_SAMPLER_GAIN,
                    9, 29, 60, 8
    EDITTEXT        IDC_SAMPLER_GAIN_VALUE,
                    80, 27, 40, 12, WS_TABSTOP
    LTEXT           "", IDC_SAMPLER_FILE_LABEL,
                    9, 49, 46, 8
    PUSHBUTTON      "", IDC_SAMPLER_BROWSE,
                    55, 47, 50, 14, 0
    EDITTEXT        IDC_SAMPLER_FILE,
                    9, 69, 160, 12, WS_TABSTOP

    DEFPUSHBUTTON   "", IDOK,
                    30, 102, 50, 14, WS_TABSTOP
    PUSHBUTTON      "", IDCANCEL,
                    90, 102, 50, 14, WS_TABSTOP
END

