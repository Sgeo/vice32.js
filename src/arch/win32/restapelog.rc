// Tape log specific resources.
//

// template of dialog for all languages

IDD_TAPELOG_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 240, 110
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    AUTOCHECKBOX  "", IDC_TAPELOG_ENABLE,
                  7, 7, 99, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    LTEXT         "", IDC_TAPELOG_DESTINATION_LABEL,
                  9, 24, 30, 8
    COMBOBOX      IDC_TAPELOG_DESTINATION,
                  45, 22, 100, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT         "", IDC_TAPELOG_FILE_LABEL,
                  9, 44, 30, 8
    PUSHBUTTON    "", IDC_TAPELOG_BROWSE,
                  45, 42, 50, 14, 0
    EDITTEXT      IDC_TAPELOG_FILE,
                  9, 58, 160, 12, WS_TABSTOP

    DEFPUSHBUTTON "", IDOK,
                  30, 90, 50, 14, WS_TABSTOP
    PUSHBUTTON    "", IDCANCEL,
                  90, 90, 50, 14, WS_TABSTOP
END


// -----------------------------------------------------------------------------

