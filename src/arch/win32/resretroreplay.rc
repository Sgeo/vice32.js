// template of dialog for all languages

IDD_RR_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 120
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    AUTOCHECKBOX    "", IDC_RR_FLASH_JUMPER,
                    7, 7, 75, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    AUTOCHECKBOX    "", IDC_RR_BANK_JUMPER,
                    7, 22, 75, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    LTEXT           "", IDC_RR_REVISION_LABEL,
                    9, 39, 53, 8
    COMBOBOX        IDC_RR_REVISION,
                    66, 37, 50, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    AUTOCHECKBOX    "", IDC_RR_SAVE_WHEN_CHANGED,
                    7, 50, 107, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    LTEXT           "", IDC_RR_CLOCKPORT_DEVICE_LABEL,
                    9, 70, 53, 8
    COMBOBOX        IDC_RR_CLOCKPORT_DEVICE,
                    66, 68, 50, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP

    DEFPUSHBUTTON   "", IDOK,
                    30, 115, 50, 14, WS_TABSTOP
    PUSHBUTTON      "", IDCANCEL,
                    90, 115, 50, 14, WS_TABSTOP
END

