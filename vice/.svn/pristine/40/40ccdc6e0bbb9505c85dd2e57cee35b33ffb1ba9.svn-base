// template of dialog for all languages

IDD_KEYMAP_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 122
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT           "", IDC_KEYMAP_INDEX_LABEL,
                    9, 9, 50, 8
    COMBOBOX        IDC_KEYMAP_INDEX,
                    65, 7, 80, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT           "", IDC_KEYMAP_MAPPING_LABEL,
                    9, 24, 50, 8
    COMBOBOX        IDC_KEYMAP_MAPPING,
                    65, 22, 80, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT           "", IDC_KEYMAP_SYM_USER_FILE_LABEL,
                    9, 39, 46, 8
    PUSHBUTTON      "", IDC_KEYMAP_SYM_USER_BROWSE,
                    55, 37, 50, 14, 0
    EDITTEXT        IDC_KEYMAP_SYM_USER_FILE,
                    9, 54, 160, 12, WS_TABSTOP

    LTEXT           "", IDC_KEYMAP_POS_USER_FILE_LABEL,
                    9, 69, 46, 8
    PUSHBUTTON      "", IDC_KEYMAP_POS_USER_BROWSE,
                    55, 67, 50, 14, 0
    EDITTEXT        IDC_KEYMAP_POS_USER_FILE,
                    9, 84, 160, 12, WS_TABSTOP

    DEFPUSHBUTTON   "", IDOK,
                    30, 102, 50, 14, WS_TABSTOP
    PUSHBUTTON      "", IDCANCEL,
                    90, 102, 50, 14, WS_TABSTOP
END

