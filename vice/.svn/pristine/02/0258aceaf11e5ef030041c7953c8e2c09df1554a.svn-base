// C64 Memory Expansion Hacks (C64 256K/+60K/+256K) specific resources.
//

// template of dialog for all languages

IDD_C64_MEMORY_HACKS_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 168
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT         "", IDC_C64_MEMORY_HACKS_DEVICE_LABEL,
                  9, 9, 30, 8
    COMBOBOX      IDC_C64_MEMORY_HACKS_DEVICE,
                  45, 7, 50, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP

    LTEXT         "", IDC_C64_256K_BASE_LABEL,
                  9, 24, 30, 8
    COMBOBOX      IDC_C64_256K_BASE,
                  45, 22, 50, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT         "", IDC_C64_256K_FILE_LABEL,
                  9, 41, 30, 8
    PUSHBUTTON    "", IDC_C64_256K_BROWSE,
                  45, 37, 50, 14, 0
    EDITTEXT      IDC_C64_256K_FILE,
                  9, 53, 160, 12, ES_AUTOHSCROLL | WS_TABSTOP

    LTEXT         "", IDC_PLUS60K_BASE_LABEL,
                  9, 69, 30, 8
    COMBOBOX      IDC_PLUS60K_BASE,
                  45, 67, 50, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT         "", IDC_PLUS60K_FILE_LABEL,
                  9, 86, 30, 8
    PUSHBUTTON    "", IDC_PLUS60K_BROWSE,
                  45, 82, 50, 14, 0
    EDITTEXT      IDC_PLUS60K_FILE,
                  9, 98, 160, 12, ES_AUTOHSCROLL | WS_TABSTOP

    LTEXT         "", IDC_PLUS256K_FILE_LABEL,
                  9, 117, 30, 8
    PUSHBUTTON    "", IDC_PLUS256K_BROWSE,
                  45, 113, 50, 14, 0
    EDITTEXT      IDC_PLUS256K_FILE,
                  9, 129, 160, 12, ES_AUTOHSCROLL | WS_TABSTOP

    DEFPUSHBUTTON "", IDOK,
                  30, 148, 50, 14, WS_TABSTOP
    PUSHBUTTON    "", IDCANCEL,
                  90, 148, 50, 14, WS_TABSTOP
END

