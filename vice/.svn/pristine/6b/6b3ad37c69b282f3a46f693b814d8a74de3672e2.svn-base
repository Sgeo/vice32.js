// Plus4 Memory Expansion Hacks (CSORY 256K/HANNES 256K/1024K/4096K) specific resources.
//

// template of dialog for all languages

IDD_PLUS4_MEMORY_HACKS_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 230, 70
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT         "", IDC_PLUS4_MEMORY_HACKS_DEVICE_LABEL,
                  9, 9, 30, 8
    COMBOBOX      IDC_PLUS4_MEMORY_HACKS_DEVICE,
                  45, 7, 90, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP

    DEFPUSHBUTTON "", IDOK,
                  30, 50, 50, 14, WS_TABSTOP
    PUSHBUTTON    "", IDCANCEL,
                  90, 50, 50, 14, WS_TABSTOP
END

