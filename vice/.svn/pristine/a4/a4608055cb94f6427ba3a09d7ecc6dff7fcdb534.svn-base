// Tape cartridge settings dialog box
//

// template of dialog for all languages

IDD_TAPECART_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 210, 130
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    AUTOCHECKBOX    "", IDC_TAPECART_ENABLE,
                    7, 7, 140, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    AUTOCHECKBOX    "", IDC_TAPECART_SAVE_ON_CHANGE,
                    7, 17, 140, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    AUTOCHECKBOX    "", IDC_TAPECART_OPTIMIZE_ON_CHANGE,
                    7, 27, 140, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    LTEXT           "", IDC_TAPECART_LOG_LEVEL_LABEL,
                    7, 39, 50, 8
    COMBOBOX        IDC_TAPECART_LOG_LEVEL,
                    65, 37, 148, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT           "", IDC_TAPECART_FILE_LABEL,
                    7, 56, 50, 8
    PUSHBUTTON      "", IDC_TAPECART_BROWSE,
                    65, 52, 40, 14, WS_TABSTOP
    EDITTEXT        IDC_TAPECART_FILE,
                    7, 68, 190, 14, WS_TABSTOP
    PUSHBUTTON      "", IDC_TAPECART_FLUSH,
                    138, 85, 60, 14, WS_TABSTOP

    DEFPUSHBUTTON   "", IDOK,
                    50, 110, 50, 14, WS_TABSTOP
    PUSHBUTTON      "", IDCANCEL,
                    110, 110, 50, 14, WS_TABSTOP
END

// -----------------------------------------------------------------------------

// original and default ENGLISH text

STRINGTABLE
LANGUAGE LANG_ENGLISH, SUBLANG_NEUTRAL
BEGIN
  IDS_TAPECART_CAPTION            "TCRT Settings"
  IDS_TAPECART_ENABLE             "Enable tapecart"
  IDS_TAPECART_SAVE_ON_CHANGE     "Save TCRT data when changed"
  IDS_TAPECART_OPTIMIZE_ON_CHANGE "Optimize TCRT data when changed"
  IDS_TAPECART_LOG_LEVEL          "Log level:"
  IDS_TAPECART_LOG_LEVEL_0        "0 (errors only)"
  IDS_TAPECART_LOG_LEVEL_1        "1 (0 plus mode changes and command bytes)"
  IDS_TAPECART_LOG_LEVEL_2        "2 (1 plus command parameter details)"
  IDS_TAPECART_FILE               "TCRT File:"
  IDS_TAPECART_SELECT_FILE        "Select tapecart file"
  IDS_TAPECART_FLUSH              "Flush"
END

