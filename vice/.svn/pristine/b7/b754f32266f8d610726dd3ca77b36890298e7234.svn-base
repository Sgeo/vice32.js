// PET Colour specific resources.
//

// template of dialog for all languages

IDD_PETCOLOUR_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 110
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT         "", IDC_PETCOLOUR_TYPE_LABEL,
                  9, 9, 50, 8
    COMBOBOX      IDC_PETCOLOUR_TYPE,
                  65, 7, 50, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT         "", IDC_PETCOLOUR_BG,
                  9, 29, 60, 8
    EDITTEXT      IDC_PETCOLOUR_BG_VALUE,
                  80, 28, 20, 12, WS_TABSTOP

    DEFPUSHBUTTON "", IDOK,
                  30, 90, 50, 14, WS_TABSTOP
    PUSHBUTTON    "", IDCANCEL,
                  90, 90, 50, 14, WS_TABSTOP
END

