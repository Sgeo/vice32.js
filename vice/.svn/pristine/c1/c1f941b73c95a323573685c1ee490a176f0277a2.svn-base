// template of dialog for all languages

IDD_ETHERNET_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 310, 100
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    LTEXT         "", IDC_ETHERNET_SETTINGS_INTERFACE_T,
                  9, 7, 30, 8
    COMBOBOX      IDC_ETHERNET_SETTINGS_INTERFACE,
                  45, 5, 240, 80, CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT         "", IDC_ETHERNET_SETTINGS_INTERFACE_NAME,
                  9, 27, 250, 8
    LTEXT         "", IDC_ETHERNET_SETTINGS_INTERFACE_DESC,
                  9, 43, 250, 8
    DEFPUSHBUTTON "", IDOK,
                  20, 58, 50, 14, WS_TABSTOP
    PUSHBUTTON    "", IDCANCEL,
                  80, 58, 50, 14, WS_TABSTOP
END

