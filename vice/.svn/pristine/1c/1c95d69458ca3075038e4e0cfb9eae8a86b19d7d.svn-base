// template of dialog for all languages

IDD_RRNETMK3_SETTINGS_DIALOG DIALOG DISCARDABLE 0, 0, 180, 100
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
FONT 8, "MS Sans Serif"
BEGIN
    AUTOCHECKBOX    "", IDC_RRNETMK3_FLASH_JUMPER,
                    7, 7, 75, 10, BS_AUTOCHECKBOX | WS_TABSTOP
    AUTOCHECKBOX    "", IDC_RRNETMK3_SAVE_WHEN_CHANGED,
                    7, 22, 107, 10, BS_AUTOCHECKBOX | WS_TABSTOP

    DEFPUSHBUTTON   "", IDOK,
                    30, 85, 50, 14, WS_TABSTOP
    PUSHBUTTON      "", IDCANCEL,
                    90, 85, 50, 14, WS_TABSTOP
END

